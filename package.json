{
  "name": "json-schema-toolbox",
  "version": "1.0.4",
  "description": "json schema toolbox",
  "main": "dist/json-schema-toolbox.js",
  "types": "dist/json-schema-toolbox.d.ts",
  "scripts": {
    "preinstall": "rollup -v",
    "build:dev": "rollup -c",
