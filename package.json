  "bugs": {
    "url": "https://github.com/paullasarev/json-schema-toolbox/issues"
  },
  "homepage": "https://github.com/paullasarev/json-schema-toolbox#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.15.8",
    "jest": "^27.2.4",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
